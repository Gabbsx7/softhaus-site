{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "static"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "routes": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/cases",
      "destination": "/cases/index.html"
    },
    {
      "source": "/cases/autoplaystudio",
      "destination": "/cases/autoplaystudio/index.html"
    },
    {
      "source": "/cases/dochub",
      "destination": "/cases/dochub/index.html"
    },
    {
      "source": "/cases/phavo",
      "destination": "/cases/phavo/index.html"
    }
  ]
} 