{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/cases",
      "dest": "/cases/index.html"
    },
    {
      "src": "/cases/autoplaystudio",
      "dest": "/cases/autoplaystudio/index.html"
    },
    {
      "src": "/cases/dochub",
      "dest": "/cases/dochub/index.html"
    },
    {
      "src": "/cases/phavo",
      "dest": "/cases/phavo/index.html"
    }
  ]
} 